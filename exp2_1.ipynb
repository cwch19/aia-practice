    "import numpy as np\n",
    "def exam_1(mylist):\n",
    "    a = np.array(mylist[0])\n",
    "    b = np.array(mylist[1])\n",
    "    c = np.array(mylist[2])\n",
    "    ab = b - a\n",
    "    ac = c - a\n",
    "    ans = ~(ab[0] / ab[1] == ac[0] / ac[1])\n",
    "    return ans"
   ]
